<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".Shopper_Activity_User_Profile">

    <View
        android:id="@+id/rectangle_1"
        android:layout_width="0dp"
        android:layout_height="248dp"
        android:background="#C9284F"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <View
        android:id="@+id/rectangle_9"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/rectangle_9"
        app:layout_constraintTop_toBottomOf="@id/rectangle_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <View
        android:id="@+id/rectangle_90"
        android:layout_width="319dp"
        android:layout_height="188dp"
        android:layout_marginTop="177dp"
        android:layout_marginStart="45dp"
        android:background="#C9284F"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/rectangle"
        android:layout_width="126dp"
        android:layout_height="119dp"
        android:layout_marginTop="130dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:src="@drawable/ic_user" />

    <TextView
        android:id="@+id/realiza_tu_"
        android:layout_width="150dp"
        android:layout_height="32dp"
        android:layout_marginTop="14dp"
        android:gravity="center_vertical"
        android:lineSpacingExtra="-7sp"
        android:text="Tu perfil "
        android:textAppearance="@style/realiza_tu_"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/btn_regresar"
        android:layout_width="34dp"
        android:layout_height="20dp"
        android:layout_marginTop="21dp"
        android:layout_marginStart="7dp"
        android:src="@drawable/frame"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    <Button
        android:id="@+id/btn_mi_cuenta"
        android:layout_width="296dp"
        android:layout_height="48dp"
        android:layout_marginTop="408dp"
        android:backgroundTint="#E9E9E9"
        android:text="Mi Cuenta"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="50dp"
        android:layout_marginTop="408dp"
        android:layout_marginEnd="56dp"
        android:src="@drawable/flechaderecha"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/pago"
        android:layout_width="60dp"
        android:layout_height="31dp"
        android:layout_marginTop="778dp"
        android:gravity="center_vertical"
        android:text="@string/pago"
        android:textAppearance="@style/pago"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:layout_width="147dp"
        android:layout_height="20dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="288dp"
        android:gravity="top"
        android:lineSpacingExtra="2sp"
        android:text="# de perfil "
        android:textAppearance="@style/actualmente"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/comprador"
        android:layout_width="147dp"
        android:layout_height="20dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="324dp"
        android:gravity="top"
        android:lineSpacingExtra="0sp"
        android:text="314 657 3810"
        android:textAppearance="@style/comprador"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/user"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginStart="176dp"
        android:layout_marginTop="248dp"
        android:gravity="top"
        android:lineSpacingExtra="0sp"
        android:text="@string/user"
        android:textAppearance="@style/user"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:layout_width="290dp"
        android:layout_height="2dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="276dp"
        android:background="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:layout_width="296dp"
        android:layout_height="48dp"
        android:layout_marginTop="470dp"
        android:text="Documentación"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:backgroundTint="#E9E9E9"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


    <ImageView
        android:layout_width="30dp"
        android:layout_height="50dp"
        android:layout_marginTop="468dp"
        android:layout_marginEnd="56dp"
        android:src="@drawable/flechaderecha"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:layout_width="296dp"
        android:layout_height="48dp"
        android:layout_marginTop="530dp"
        android:textColor="@color/black"
        android:text="Ayuda"
        android:textSize="20sp"
        android:backgroundTint="#E9E9E9"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


    <ImageView
        android:layout_width="30dp"
        android:layout_height="50dp"
        android:layout_marginTop="528dp"
        android:layout_marginEnd="64dp"
        android:src="@drawable/soporte"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:layout_width="296dp"
        android:layout_height="48dp"
        android:layout_marginTop="590dp"
        android:text="Terminos y Condiciones"
        android:textSize="20sp"
        android:textColor="@color/black"
        android:backgroundTint="#E9E9E9"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


    <ImageView
        android:layout_width="30dp"
        android:layout_height="50dp"
        android:layout_marginTop="588dp"
        android:layout_marginEnd="64dp"
        android:src="@drawable/flechaderecha"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="205dp"
        android:layout_height="38dp"
        android:layout_marginTop="655dp"
        android:gravity="center_vertical"
        android:text="Cerrar sesión"
        android:textAppearance="@style/comunicate_"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />



    <Button
        android:id="@+id/btn_modo_vendedor"
        android:text="Iniciar modo vendedor"
        android:textSize="20sp"
        android:textColor="@color/black"
        android:layout_width="296dp"
        android:layout_height="48dp"
        android:layout_marginTop="650dp"
        android:backgroundTint="#E9E9E9"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="50dp"
        android:layout_marginTop="648dp"
        android:layout_marginEnd="64dp"
        android:src="@drawable/logo_boxi_circulo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>

